#!/bin/bash

#patterns and texts for 10^6
./a.out A B 1 1000000 0
./a.out B C 10 100000 1
./a.out C D 100 10000 2
./a.out D E 1000 1000 3

#patterns and texts for 10^8
./a.out E F 4000 25000 4
./a.out F G 10 10000000 5
./a.out G H 100 1000000 6
./a.out H I 1000 100000 7

#patterns and texts for 10^10
./a.out I J 40000 250000 8
./a.out J K 50000 200000 9
./a.out K L 12500 800000 10
./a.out L M 1000 10000000 11

#patterns and texts for 10^4
./a.out M N 1 10000 12
./a.out N O 10 1000 13
./a.out O P 100 100 14
./a.out P Q 1000 10 15

#patterns and texts for 10^2
./a.out Q R 1 100 16
./a.out R S 10 10 17
./a.out S T 100 1 18
./a.out T U 4 25 19

