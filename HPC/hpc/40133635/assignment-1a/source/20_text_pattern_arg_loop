#!/bin/bash

#patterns and texts for 10^6
./a.out A B 1 1000000 0
./a.out B C 10 100000 1
./a.out C D 100 10000 2
./a.out D E 1000 1000 3

#patterns and texts for 10^8
./a.out E F 10 10000000 4
./a.out F G 100 1000000 5
./a.out G H 500 200000 6
./a.out H I 1000 100000 7

#patterns and texts for 10^10
./a.out I J 500 20000000 8
./a.out J K 1000 10000000 9
./a.out K L 5000 2000000 10
./a.out L M 10000 1000000 11

#patterns and texts for 10^4
./a.out M N 1 10000 12
./a.out N O 5 2000 13
./a.out O P 10 1000 14
./a.out P Q 100 100 15

#patterns and texts for 10^2
./a.out Q R 1 100 16
./a.out R S 2 50 17
./a.out S T 5 20 18
./a.out T U 10 10 19

